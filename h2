spring:
  config:
    activate:
      on-profile: test

  batch:
    job:
      names: ${job.name:NONE}
      enabled: false
  h2:
    console:
      enabled: true
  jpa:
    database: h2
    generate-ddl: off
    properties:
      hibernate:
        dialect: org.hibernate.dialect.MariaDB106Dialect
  datasource:
    url: jdbc:h2:tcp://localhost:9092/~/test;
    username: sa
    password:
  sql:
    init:
      mode:
      schema-locations: classpath:schema-h2.sql
      data-locations: classpath:data-h2.sql